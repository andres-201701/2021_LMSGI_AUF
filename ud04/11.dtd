<?xml version='1.0' encoding='UTF-8'?>
<!ELEMENT catalogo (portatil*)>
<!ELEMENT portatil (referencia,nombre,fabricante,procesador,ram,disco,pantalla,grÃ¡fica,foto*)>
<!ELEMENT referencia (#PCDATA)>
